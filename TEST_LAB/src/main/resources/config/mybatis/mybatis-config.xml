<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN" "http://mybatis.org/dtd/mybatis-3-config.dtd">

<configuration>
	<!--
	properties,
	settings,
	typeAliases,
	typeHandlers,
	objectFactory,
	objectWrapperFactory,
	plugins,
	environments,
	databaseIdProvider,
	mappers

	순서 지키지 않을시
	요소 유형 "configuration"의 콘텐츠는 "(properties?,settings?,typeAliases?,typeHandlers?,objectFactory?,objectWrapperFactory?,
	plugins?,environments?,databaseIdProvider?,mappers?)"과(와) 일치해야 합니다
	 -->
	<settings>
		<setting name="cacheEnabled" value="true" />
		<setting name="lazyLoadingEnabled" value="true" />
		<setting name="multipleResultSetsEnabled" value="true" />
		<setting name="defaultExecutorType" value="REUSE" />
		<setting name="defaultStatementTimeout" value="25" />
		<setting name="logImpl" value="NO_LOGGING" />
		<setting name="mapUnderscoreToCamelCase" value="true" />
		<setting name="jdbcTypeForNull" value="NULL" />
		<setting name="callSettersOnNulls" value="true"/>
	</settings>

	<typeAliases>
		<typeAlias alias="map" type="java.util.Map" />
		<typeAlias alias="list" type="java.util.List" />
		<typeAlias alias="UserVo" type="kr.co.aspn.vo.UserVO" />	
		<typeAlias alias="UserManageVO" type="kr.co.aspn.vo.UserManageVO" />
		<typeAlias alias="MaterialVo" type="kr.co.aspn.vo.MaterialVO" />	
		<typeAlias alias="PlantVO" type="kr.co.aspn.vo.PlantVO" />
		<typeAlias alias="CompanyVO" type="kr.co.aspn.vo.CompanyVO" />	
		<typeAlias alias="UnitVO" type="kr.co.aspn.vo.UnitVO" />	
		<typeAlias alias="CodeGroupVO" type="kr.co.aspn.vo.CodeGroupVO" />
		<typeAlias alias="CodeItemVO" type="kr.co.aspn.vo.CodeItemVO" />	
		<typeAlias alias="UnitVO" type="kr.co.aspn.vo.UnitVO" />	
		<typeAlias alias="PageVO" type="kr.co.aspn.vo.PageVO" />	
		<typeAlias alias="SearchVO" type="kr.co.aspn.vo.SearchVO" />
		<typeAlias alias="ApprovalHeaderVO" type="kr.co.aspn.vo.ApprovalHeaderVO" />			
		<typeAlias alias="ApprovalItemVO" type="kr.co.aspn.vo.ApprovalItemVO" />
		<typeAlias alias="ApprovalReferenceVO" type="kr.co.aspn.vo.ApprovalReferenceVO" />
		<typeAlias alias="ReportVO" type="kr.co.aspn.vo.ReportVO" />
		<typeAlias alias="QAReportVO" type="kr.co.aspn.vo.QAReportVO" />
		<typeAlias alias="ProcessLineVO" type="kr.co.aspn.vo.ProcessLineVO" />
		<typeAlias alias="FileVO" type="kr.co.aspn.vo.FileVO" />
		<typeAlias alias="ProxyVO" type="kr.co.aspn.vo.ProxyVO" />
		<typeAlias alias="MaterialManagementVO" type="kr.co.aspn.vo.MaterialManagementVO" />
		<typeAlias alias="MaterialManagementItemVO" type="kr.co.aspn.vo.MaterialManagementItemVO" />

		
		<typeAlias alias="TrialProductionReportVO" type="kr.co.aspn.vo.TrialProductionReportVO" /> <!-- 시생산 보고서 -->
		<typeAlias alias="TrialReportHeader" type="kr.co.aspn.vo.TrialReportHeader"/> <!-- 시생산 보고서 by guanghai.cui -->
		<typeAlias alias="TrialReportComment" type="kr.co.aspn.vo.TrialReportComment"/>
		<typeAlias alias="TrialReportFile" type="kr.co.aspn.vo.TrialReportFile"/>

	</typeAliases>
</configuration>



